{
  "name": "bestbeds-backend-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "backpack build",
    "dev": "backpack",
    "node": "nodemon ./src/index.js",
    "lint": "prettier --write .",
    "start": "node ./build/main.js",
    "test": "yarn test:unit && yarn test:component",
    "test:unit": "yarn build && mocha --timeout 30000 ./test/test.spec.js --exit",
    "test:unit:coverage": "yarn build && nyc mocha --timeout 30000 ./test/test.spec.js --exit",
    "test:component": "yarn build && mocha --no-timeout ./test/db/*.spec.js --exit",
    "test:component:coverage": "yarn build && nyc mocha --no-timeout ./test/db/*.spec.js --exit",
    "test:e2e": "yarn build && mocha --no-timeout ./test/e2e/*.spec.js --exit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.1",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "backpack-core": "^0.8.4",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.6.1"
  }
}
